<div id="tank-<%=tank.id%>">
  <div class="col-md-12">
    <div class="card">
      <div class="card-header">
        <h5 class="card-title"><%=tank.name%></h5>
        <p class="card-category"><%=tank.description%></p>
        <%= link_to "dashboard", dashboard_path(@user, tank), method: :get, class:"btn btn-warning btn-sm"%>
        <%= link_to("Edit",edit_user_tank_path(@user,tank), class: "btn btn-primary btn-sm", remote: true) %>
        <%= link_to 'Delete', user_tank_path(@user,tank), method: :delete, remote: true, data: { confirm: 'Are you sure?' },class: "btn btn-danger btn-sm" %>
      </div>
    </div>
  </div>
</div>
