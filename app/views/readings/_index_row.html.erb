<% if reading.value <= reading.sensor.low_value || reading.value >= reading.sensor.high_value %>
<tr id="reading-<%= reading.id %>" class="table-danger">
<%elsif reading.value == reading.sensor.goal_value%>
<tr id="reading-<%= reading.id %>" class="table-success">
<%else%>
  <tr id="reading-<%= reading.id %>">
<%end%>
  <td class="reading-name"><%= reading.value%></td>
  <td class="reading-sensor_goal_value"><%= reading.sensor.goal_value %></td>
  <td class="reading-sensor"><%=reading.sensor.name%></td>
  <td class="reading-date"><%= reading.date&.strftime("%m/%d/%Y")%></td>
  <td class="reading-date"><%= reading.created_at&.strftime("%m/%d/%Y %I:%M%p")%></td>
  <td>
    <%= link_to(edit_user_tank_reading_path(@user,reading.tank,reading), class: "btn btn-primary btn-sm", remote: true) do %>
      <i class="fa fa-pencil"></i>
    <%end%>

    <%= link_to(user_tank_reading_path(@user,reading.tank,reading), method: :delete, remote: true, data: { confirm: 'Are you sure?' },class: "btn btn-danger btn-sm") do %>
      <i class="fa fa-trash"></i>
    <%end%>
  </td>
</tr>