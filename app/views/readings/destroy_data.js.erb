var length = '<%=@reading.data["readings"].length%>';
if (Number(length) == 0){
    $('#accordion-<%=@reading.id%>').remove();    
}else{
    $('#accordion_header-<%=@reading.id%>').replaceWith("<%= j render partial: 'acordion_header', locals: {reading: @reading} %>");
    $('#reading-<%=@reading.id%>-<%=@index%>').remove();
}
