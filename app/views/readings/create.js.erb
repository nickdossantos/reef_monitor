var myEle = document.getElementById("accordion-<%=@reading.id%>");
if (myEle){
    $('#accordion-<%=@reading.id%>').replaceWith("<%= j render partial: 'reading_acordion', locals: {reading: @reading} %>");
}else{
    $('#card-body-acordeon').append("<%= j render partial: 'reading_acordion', locals: {reading: @reading} %>");
}
$('#readingModal').modal('toggle');